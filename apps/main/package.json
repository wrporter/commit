{
  "name": "commit-main",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": "20",
    "npm": ">=10"
  },
  "imports": {
    "~/*": "./app/*",
    "#server/*": "./server/*",
    "#tests/*": "./tests/*"
  },
  "scripts": {
    "build": "NODE_ENV=production run-s build:*",
    "build:react-router": "react-router build",
    "build:server": "tsx scripts/build-server.ts",
    "ci": "run-p lint test:coverage build && npm run typecheck",
    "clean": "rm -rf junit.xml coverage build public/build",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "dev": "npm run dev:base -- server/main.ts",
    "dev:base": "NODE_ENV=development tsx watch --env-file=.env --clear-screen=false --ignore \".cache/**\" --ignore \"app/**\" --ignore \"vite.config.ts.timestamp-*\" --ignore \"build/**\" --ignore \"node_modules/**\"",
    "dev:debug": "npm run dev:base -- --inspect server/main.ts",
    "dev:trace": "npm run dev:base -- --require=./server/opentelemetry.cjs server/main.ts",
    "docker:build": "SCOPE=main .ci/build.sh",
    "docker:deploy": "SCOPE=main .ci/deploy.sh",
    "docker:run": "SCOPE=main .ci/run-local.sh",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "npm run lint -- --fix",
    "run": "node -r esbuild-runner/register --env-file=.env",
    "start": "NODE_ENV=production node --enable-source-maps --env-file=.env --require=./server-build/opentelemetry.cjs ./server-build/main.js",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "react-router typegen && tsc"
  },
  "dependencies": {
    "@epic-web/client-hints": "1.3.5",
    "@epic-web/remember": "1.1.0",
    "@heroicons/react": "2.2.0",
    "@internationalized/date": "3.7.0",
    "@heroui/react": "2.6.14",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/auto-instrumentations-node": "0.55.3",
    "@opentelemetry/sdk-node": "0.57.1",
    "@react-router/express": "7.1.3",
    "@react-router/node": "7.1.3",
    "@rvf/react-router": "7.0.2",
    "@rvf/zod": "7.0.1",
    "@wesp-up/express": "3.0.1",
    "bcryptjs": "2.4.3",
    "decimal.js": "10.4.3",
    "drizzle-orm": "0.38.4",
    "express": "4.21.2",
    "framer-motion": "12.0.3",
    "isbot": "5.1.21",
    "postgres": "3.4.5",
    "prom-client": "15.1.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router": "7.1.3",
    "remix-auth": "4.1.0",
    "remix-auth-form": "3.0.0",
    "remix-auth-oauth2": "3.2.2",
    "source-map-support": "0.5.21",
    "tailwind-merge": "2.6.0",
    "tailwind-variants": "0.3.1",
    "tiny-invariant": "1.3.3",
    "uuid": "11.0.5",
    "zod": "3.24.1",
    "zod-form-data": "2.0.5"
  },
  "devDependencies": {
    "@react-router/dev": "7.1.3",
    "@react-router/remix-routes-option-adapter": "7.1.3",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@types/bcryptjs": "2.4.6",
    "@types/express": "4.17.21",
    "@types/fs-extra": "11.0.4",
    "@types/node": "22.10.10",
    "@types/pg": "8.11.10",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@types/source-map-support": "0.5.10",
    "@vitejs/plugin-react": "4.3.4",
    "@vitest/coverage-v8": "3.0.4",
    "@wesp-up/eslint-config-react": "2.1.0",
    "autoprefixer": "10.4.20",
    "drizzle-kit": "0.30.2",
    "esbuild": "0.24.2",
    "eslint": "9.18.0",
    "fs-extra": "11.3.0",
    "jsdom": "26.0.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.5.1",
    "remix-flat-routes": "0.7.1",
    "tailwindcss": "3.4.17",
    "tsx": "4.19.2",
    "typescript": "5.7.3",
    "vite": "6.0.11",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.4"
  }
}