{
  "name": "commit-main",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "imports": {
    "#app/*": "./app/*",
    "#server/*": "./server/*",
    "#tests/*": "./tests/*"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix vite:build --sourcemapClient --sourcemapServer",
    "build:server": "tsx other/build-server.ts",
    "ci": "run-p lint test:coverage build && npm run typecheck",
    "clean": "rm -rf junit.xml coverage build public/build",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:update": "prisma db push && prisma generate",
    "dev": "npm run dev:base -- server/main.ts",
    "dev:base": "tsx watch --env-file=.env --clear-screen=false --ignore \".cache/**\" --ignore \"app/**\" --ignore \"vite.config.ts.timestamp-*\" --ignore \"build/**\" --ignore \"node_modules/**\"",
    "dev:debug": "npm run dev:base -- --inspect server/main.ts",
    "docker:build": "cd ../.. && SCOPE=@wesp-up/commit-app APP_NAME=commit-app .ci/app/build.sh",
    "docker:deploy": "cd ../.. && APP_NAME=commit-app WORKSPACE_PATH=apps/commit-app REMOTE_APP_DIRECTORY=commit .ci/app/deploy.sh",
    "docker:run": "cd ../.. && APP_NAME=commit-app .ci/app/run.sh yarn workspace @wesp-up/commit-app start",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "npm run lint -- --fix",
    "run": "node -r esbuild-runner/register --env-file=.env",
    "start": "NODE_ENV=production node server-build/main.js",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@epic-web/remember": "1.1.0",
    "@headlessui/react": "2.1.2",
    "@heroicons/react": "2.1.4",
    "@nextui-org/react": "2.4.2",
    "@prisma/client": "5.16.1",
    "@radix-ui/react-avatar": "1.1.0",
    "@radix-ui/react-checkbox": "1.1.1",
    "@remix-run/express": "2.10.2",
    "@remix-run/node": "2.10.2",
    "@remix-run/react": "2.10.2",
    "@remix-validated-form/with-zod": "2.0.7",
    "@wesp-up/express": "1.0.1",
    "@wesp-up/express-remix": "1.0.1",
    "@wesp-up/ui": "*",
    "bcryptjs": "2.4.3",
    "drizzle-orm": "0.31.2",
    "express": "4.19.2",
    "framer-motion": "11.2.13",
    "isbot": "5.1.11",
    "postgres": "3.4.4",
    "prom-client": "15.1.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "remix-auth": "3.7.0",
    "remix-auth-form": "1.5.0",
    "remix-auth-google": "2.0.0",
    "remix-validated-form": "5.1.5",
    "source-map-support": "0.5.21",
    "tailwind-merge": "2.3.0",
    "tailwind-variants": "0.2.1",
    "tiny-invariant": "1.3.3",
    "zod": "3.23.8",
    "zod-form-data": "2.0.2"
  },
  "devDependencies": {
    "@epic-web/config": "1.11.2",
    "@remix-run/dev": "2.10.2",
    "@testing-library/jest-dom": "6.4.6",
    "@testing-library/react": "16.0.0",
    "@types/bcryptjs": "2.4.6",
    "@types/express": "4.17.21",
    "@types/fs-extra": "11.0.4",
    "@types/node": "20.14.10",
    "@types/pg": "8.11.6",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/source-map-support": "0.5.10",
    "@vitejs/plugin-react": "4.3.1",
    "@vitest/coverage-v8": "1.6.0",
    "autoprefixer": "10.4.19",
    "drizzle-kit": "0.22.8",
    "esbuild": "0.23.0",
    "eslint": "9.6.0",
    "eslint-plugin-unused-imports": "4.0.0",
    "fs-extra": "11.2.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.39",
    "prisma": "5.16.1",
    "tailwindcss": "3.4.4",
    "tsx": "4.16.2",
    "typescript": "5.5.3",
    "vite": "5.3.3",
    "vitest": "1.6.0"
  }
}
